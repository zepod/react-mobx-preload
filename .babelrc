{
  "presets": [
    "es2015-loose",
    "stage-0",
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators-legacy",
    "transform-react-jsx",
    "transform-runtime",
    "transform-regenerator",
    "add-module-exports"
  ],
  "env": {
    "build": {
      "presets": [
        "es2015-loose",
        "stage-0",
        "react"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-react-jsx",
        "transform-runtime",
        "transform-regenerator",
        "add-module-exports"
      ]
    },
    "test": {
      "presets": [
        "es2015-loose",
        "stage-0",
        "react"
      ],
      "plugins": [
        "syntax-async-functions",
        "syntax-jsx",
        "transform-react-constant-elements",
        "transform-class-properties",
        "transform-decorators-legacy",
        "transform-react-jsx",
        "transform-runtime",
        "transform-regenerator",
        "add-module-exports"
      ]
    }
  }
}
